<%= form_for @customer_profile do |f| %>
  <%= f.label :firstname %>
  <%= f.text_field :firstname %>
  <%= f.label :lastname %>
  <%= f.text_field :lastname %>
  <%= f.label :about %>
  <%= f.text_area :about %>
  <%= f.label :interests %>
  <%= f.text_field :interests %>
  <%= f.submit %>
<% end %>
